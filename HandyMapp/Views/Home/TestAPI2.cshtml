
@{
    Layout = null;
}

<html>
<head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <title>Directions service</title>
    <style>
        html, body, #map-canvas {
            height: 100%;
            margin: 0px;
            padding: 0px;
        }

        #panel {
            position: absolute;
            top: 5px;
            left: 50%;
            margin-left: -180px;
            z-index: 5;
            background-color: #fff;
            padding: 5px;
            border: 1px solid #999;
        }
    </style>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDfFiQB4uFA8_lS-24Ll1EFUXxfGVGoJWs&sensor=false"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>

</head>
<body>
    <div id="console"></div>
    <center><h1>What do you want to review?</h1></center>

   
    <div id="map-canvas">

        <center>

           

        <input type="image" height="30%" width="30%" src="" onclick="location.href='@Url.Action("street_eval","Home")'"/>
        <input type="image" height="30%" width="30%" src="" onclick="location.href='@Url.Action("buildings_eval","Home")'"/>

        </center>
    </div>

</body>
</html>